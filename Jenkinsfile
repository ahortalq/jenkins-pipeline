pipeline {
    agent any
    stages {
        stage('Validate') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Compile') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Package') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Verify') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Install') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
        stage('Publish') {
            steps {
                sh 'echo "Hello World!"'
            }
        }
    }
}